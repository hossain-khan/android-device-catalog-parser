CREATE TABLE "device" (
  "_id" INTEGER NOT NULL ON CONFLICT REPLACE PRIMARY KEY AUTOINCREMENT,
  "brand" TEXT NOT NULL,
  "device" TEXT NOT NULL,
  "manufacturer" TEXT NOT NULL,
  "model_name" TEXT,
  "ram" TEXT,
  "processor_name" TEXT
);


selectAll:
SELECT *
FROM device;

insert:
INSERT INTO device(brand, device, manufacturer, model_name, ram, processor_name)
VALUES (?, ?, ?, ?, ?, ?);